{
    "breadcrumbs.enabled": true,
    "colorInfo.fields": [ "hex", "rgb", "hsl" ],
    "editor.cursorBlinking": "solid",
    "editor.cursorStyle": "line",
    "editor.fontFamily": "Fira Code, 'Font Awesome 5 Brands',  'Font Awesome 5 Free:Solid', 'Font Awesome 5 Free',monospace, 'Apple Color Emoji'",
    "editor.fontLigatures": true,
    "editor.fontWeight": "400",
    "editor.formatOnPaste": true,
    "editor.insertSpaces": true,
    "editor.minimap.enabled": false,
    "editor.multiCursorModifier": "ctrlCmd",
    "editor.parameterHints.enabled": false,
    "editor.scrollBeyondLastLine": false,
    "editor.snippetSuggestions": "top",
    "editor.tabCompletion": "onlySnippets",
    "editor.wordWrap": "off",
    "editor.tabSize": 2,
    // "emmet.triggerExpansionOnTab": true,
    "files.defaultLanguage": "markdown",
    "git.autofetch": true,
    "java.errors.incompleteClasspath.severity": "ignore",
    "window.title": "${dirty} ${activeEditorMedium}${separator}${rootName}",
    "window.titleBarStyle": "custom",
    "window.zoomLevel": 0,
    "workbench.colorTheme": "Night Owl",
    "workbench.editor.enablePreviewFromQuickOpen": false,
    "workbench.startupEditor": "newUntitledFile",
    "[python]": {
        "editor.tabSize": 4
    },
    "files.associations": {
        "*.cshtml": "cshtml-tm"
    },
    "editor.suggestSelection": "first",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "nativescript.analytics.enabled": false,
    "python.jediEnabled": false,
    "html.format.indentInnerHtml": true,
    "java.configuration.checkProjectSettingsExclusions": false,
    "git.ignoreMissingGitWarning": true,
    "terminal.external.osxExec": "iTerm.app",
    "extensions.ignoreRecommendations": false,
    "polacode.backgroundColor": "#4ABFC1",
    "polacode.shadow": "rgba(0, 0, 0, 0.55) 0px 10px 38px",
    "terminal.integrated.shell.osx": "/bin/zsh",
    "todo-tree.tree.showScanModeButton": false,
    "todo-tree.general.tags": [
        "BUG",
        "HACK",
        "FIXME",
        "TODO",
        "XXX",
        "REVIEW"
    ],
    "todo-tree.general.tagGroups": {
    
    },
    "todo-tree.highlights.defaultHighlight": {
        "icon": "pin",
        "type": "text-and-comment",
        "foreground": "#001D33",
        "iconColour": "#91B7FB",
        "background": "#91B7FB",
        "borderRadius": 0
    },
    "todo-tree.highlights.customHighlight": {
        "FIXME": {
            "icon": "tools",
            "foreground": "#001D33",
            "iconColour": "#F0CD9F",
            "background": "#F0CD9F"
        },
        "BUG": {
            "foreground": "#001D33",
            "iconColour": "#FF7084",
            "background": "#FF7084"
        },
        "REVIEW": {
            "icon": "eye",
            "foreground": "#001D33",
            "iconColour": "#D5A6EC",
            "background": "#D5A6EC"
        }
    }
}